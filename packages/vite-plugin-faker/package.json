{
  "name": "vite-plugin-faker",
  "version": "0.1.0",
  "description": "Vite插件，用于拦截API请求并使用faker进行模拟数据",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "build": "rolldown --config rolldown.config.ts --minify"
  },
  "keywords": [
    "vite-plugin",
    "faker",
    "mock",
    "api"
  ],
  "dependencies": {
    "@baicie/faker-interceptor": "workspace:*",
    "@baicie/faker-shared": "workspace:*",
    "@baicie/faker-ui": "workspace:*",
    "@baicie/logger": "^0.1.1",
    "@faker-js/faker": "^10.1.0",
    "lodash-es": "^4.17.21",
    "lowdb": "^7.0.1"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "fs-extra": "^11.3.2",
    "rolldown": "1.0.0-beta.53",
    "rolldown-plugin-dts": "^0.18.2",
    "vite": "catalog:"
  }
}
