{
  "name": "vite-plugin-faker",
  "version": "0.1.0",
  "description": "Vite插件，用于拦截API请求并使用faker进行模拟数据",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "build": "rolldown --config rolldown.config.ts"
  },
  "keywords": [
    "vite-plugin",
    "faker",
    "mock",
    "api"
  ],
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@rolldown/pluginutils": "1.0.0-beta.26",
    "faker-ui": "workspace:*",
    "lit": "^3.3.1",
    "msw": "^2.10.3"
  },
  "peerDependencies": {
    "vite": "^7.0.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.3.0",
    "rolldown": "1.0.0-beta.26",
    "rolldown-plugin-dts": "^0.13.13"
  }
}
