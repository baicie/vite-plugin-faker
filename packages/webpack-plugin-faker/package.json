{
  "name": "webpack-plugin-faker",
  "version": "0.1.0",
  "description": "Webpack plugin for mocking API requests with faker",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "build": "rolldown --config rolldown.config.ts --minify",
    "typecheck": "tsc --incremental --noEmit"
  },
  "keywords": [
    "webpack-plugin",
    "faker",
    "mock",
    "api"
  ],
  "dependencies": {
    "@baicie/faker-interceptor": "workspace:*",
    "@baicie/faker-shared": "workspace:*",
    "@baicie/faker-core": "workspace:*",
    "@baicie/faker-ui": "workspace:*",
    "@baicie/logger": "^0.1.3",
    "@faker-js/faker": "^10.2.0",
    "chokidar": "^4.0.0",
    "co-body": "^6.2.0",
    "lodash-es": "^4.17.23",
    "lowdb": "^7.0.1",
    "qs": "^6.14.1",
    "type-is": "^1.6.18",
    "ws": "^8.19.0",
    "picocolors": "^1.1.1"
  },
  "peerDependencies": {
    "webpack": "^5.0.0",
    "webpack-dev-server": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/co-body": "^6.1.3",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.23",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.0.0",
    "@types/qs": "^6.9.15",
    "@types/type-is": "^1.6.7",
    "@types/ws": "^8.5.12",
    "fs-extra": "^11.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-dev-server": "^5.0.0"
  }
}
