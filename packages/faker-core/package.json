    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.23",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.0",
    "@types/qs": "^6.14.0",
    "@types/type-is": "^1.6.7",
