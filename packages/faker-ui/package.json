{
  "name": "@baicie/faker-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "css": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "css": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "analyze": "cross-env ANALYZE=true rolldown --config rolldown.config.ts",
    "vite": "vite",
    "build": "svelte-check && rolldown --config rolldown.config.ts --minify",
    "preview": "vite preview",
    "typecheck": "tsc --incremental --noEmit"
  },
  "dependencies": {
    "@baicie/faker-shared": "workspace:*",
    "@baicie/logger": "^0.1.3",
    "@monaco-editor/loader": "^1.7.0",
    "@vicons/ionicons5": "^0.13.0",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "lodash-es": "^4.17.22",
    "lucide-svelte": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "rolldown": "1.0.0-beta.57",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.2.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "vite": "catalog:"
