{
  "name": "@baicie/faker-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "css": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "css": "./dist/index.css"
    },
    "./dist/index.js": "./dist/index.js",
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
