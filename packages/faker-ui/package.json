{
  "name": "@baicie/faker-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "css": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "css": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "analyze": "cross-env ANALYZE=true rolldown --config rolldown.config.ts",
    "vite": "vite",
    "build": "vue-tsc -b && rolldown --config rolldown.config.ts --minify",
    "preview": "vite preview",
    "typecheck": "tsc --incremental --noEmit"
  },
  "dependencies": {
    "@baicie/faker-shared": "workspace:*",
    "@baicie/logger": "^0.1.1",
    "@monaco-editor/loader": "^1.7.0",
    "@vicons/ionicons5": "^0.13.0",
    "monaco-editor": "^0.55.1",
    "naive-ui": "^2.43.2",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "postcss-prefix-selector": "^2.1.1",
    "rolldown": "1.0.0-beta.54",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue-tsc": "^3.1.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
