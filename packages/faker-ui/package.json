{
  "name": "@baicie/faker-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "css": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "css": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "analyze": "cross-env ANALYZE=true rolldown --config rolldown.config.ts",
    "vite": "vite",
    "build": "vue-tsc -b && rolldown --config rolldown.config.ts --minify",
    "preview": "vite preview",
    "typecheck": "tsc --incremental --noEmit"
  },
  "dependencies": {
    "@baicie/faker-shared": "workspace:*",
    "@baicie/logger": "^0.1.3",
    "@monaco-editor/loader": "^1.7.0",
    "@vicons/ionicons5": "^0.13.0",
    "highlight.js": "^11.11.1",
    "monaco-editor": "^0.55.1",
    "naive-ui": "^2.43.2",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/tsconfig": "^0.8.1",
    "postcss": "^8.5.6",
    "rolldown": "1.0.0-beta.56",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "~5.9.3",
    "vite": "catalog:",
    "vue-tsc": "^3.2.1"
  }
}
