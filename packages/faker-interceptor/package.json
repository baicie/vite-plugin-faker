{
  "name": "@baicie/faker-interceptor",
  "version": "0.1.0",
  "description": "Browser-side request interceptor with Faker.js support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rolldown --config rolldown.config.ts --watch",
    "build": "rolldown --config rolldown.config.ts --minify",
    "typecheck": "tsc --incremental --noEmit && tsc -p src/worker/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@baicie/logger": "^0.1.3",
    "@faker-js/faker": "^10.1.0",
    "msw": "^2.12.4"
  },
  "devDependencies": {
    "@baicie/faker-shared": "workspace:*",
    "@types/serviceworker": "^0.0.170",
    "rolldown": "1.0.0-beta.56",
    "rollup-plugin-web-worker-loader": "^1.7.0",
    "typescript": "~5.9.3",
    "vite": "catalog:"
  }
}
