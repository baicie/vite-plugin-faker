{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Vite Plugin (server)",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect", "../../node_modules/tsx/dist/cli.cjs"],
      "args": ["vite.script.ts"],
      "cwd": "${workspaceFolder}/playground/vue-app",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
